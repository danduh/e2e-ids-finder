{
  "manifest_version": 3,
  "version": "4.2.2",
  "name": "E2E Test Helper",
  "description": "A helpful tool for locating UI elements for Cypress, Playwright, Selenium. Will create a Page Objects for by Generative AI.",
  "author": "Daniel Ostrovsky",
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": [
    "https://*.dell.com/*",
    "https://api.openai.com/*",
    "https://*.delllabs.net/*"
  ],
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  },
  "permissions": [
    "scripting",
    "activeTab",
    "storage"
  ]
}
