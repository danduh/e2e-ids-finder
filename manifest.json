{
  "manifest_version": 2,
  "version": "0.1",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "name": "E2E Test Helper :-)",
  "description": "This extension shows e2e ids, and will suggest (in near future) to add e2e ids to other elements",
  "browser_action": {
    "default_icon": "src/assets/icon.png",
    "default_popup": "src/popup/popup.html"
  },
  "options_ui": {
    "page": "src/options/options.html",
    "chrome_style": true
  },
  "icons": {
    "48": "src/assets/icon-32.png",
    "128": "src/assets/icon.png"
  },
  "permissions": [
    "https://*/",
    "contextMenus",
    "notifications",
    "declarativeContent",
    "tabs",
    "activeTab",
    "storage"
  ]
}