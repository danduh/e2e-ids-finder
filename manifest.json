{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "name": "e2e-ids-finder",
  "description": "This ectension shows e2e ids, and will suggest to add e2e ids to other elements",
  "version": "1.0",
  "browser_action": {
    "default_icon": "src/assets/icon.png",
    "default_popup": "src/popup/popup.html"
  },
  "options_ui": {
    "page": "src/options/options.html",
    "chrome_style": true
  },
  "permissions": [
    "https://*/",
    "contextMenus",
    "notifications",
    "declarativeContent",
    "tabs",
    "activeTab",
    "storage"
  ]
}